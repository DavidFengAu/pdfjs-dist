{
  "name": "@bgl/pdfjs-dist",
  "version": "2.3.200-1",
  "main": "./dist/build/pdf.js",
  "description": "Generic build of Mozilla's PDF.js library.",
  "keywords": [
    "Mozilla",
    "pdf",
    "pdf.js"
  ],
  "homepage": "http://mozilla.github.io/pdf.js/",
  "bugs": "https://github.com/mozilla/pdf.js/issues",
  "license": "Apache-2.0",
  "scripts": {
    "cpBuild": "cp -R ./build ./dist/build",
    "cpCmaps": "cp -R ./cmaps ./dist/cmaps",
    "cpImgDecoders": "cp -R ./image_decoders ./dist/image_decoders",
    "build": "rm -rf ./dist && NODE_ENV=production babel lib -d ./dist/lib && yarn cpBuild && yarn cpCmaps && yarn cpImgDecoders"
  },
  "dependencies": {
    "node-ensure": "^0.0.0",
    "worker-loader": "^2.0.0",
    "web-streams-polyfill": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.9.6",
    "@types/node": "^13.13.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidFengAu/pdfjs-dist"
  }
}
